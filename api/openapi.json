{
  "openapi": "3.1.0",
  "info": {
    "title": "BookmarkHive API",
    "version": "1.0.0",
    "description": "API for managing bookmarks, tags, and user profiles"
  },
  "servers": [
    {
      "url": "https://bookmarkhive.test",
      "description": "Local Server"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/api/auth": {
      "post": {
        "summary": "Authenticate user",
        "description": "Login with email and password to receive a JWT token",
        "tags": ["Authentication"],
        "security": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["email", "password"],
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "user@example.com"
                  },
                  "password": {
                    "type": "string",
                    "format": "password",
                    "example": "password123"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Authentication successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "description": "JWT token for authentication",
                      "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiJ9..."
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Invalid credentials"
          }
        }
      }
    },
    "/api/account": {
      "post": {
        "summary": "Register new account",
        "description": "Create a new user account",
        "tags": ["Account"],
        "security": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Account created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserOwner"
                }
              }
            }
          },
          "422": {
            "description": "Validation error or duplicate email/username"
          }
        }
      }
    },
    "/api/users/me": {
      "get": {
        "summary": "Get own profile",
        "description": "Retrieve the authenticated user's profile",
        "tags": ["Me"],
        "responses": {
          "200": {
            "description": "User profile",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "patch": {
        "summary": "Update own profile",
        "description": "Update the authenticated user's profile",
        "tags": ["Me"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserUpdate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Profile updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "delete": {
        "summary": "Delete own profile",
        "description": "Delete the authenticated user's account",
        "tags": ["Me"],
        "responses": {
          "204": {
            "description": "Account deleted successfully"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/api/users/me/bookmarks": {
      "get": {
        "summary": "List own bookmarks",
        "description": "Retrieve all bookmarks owned by the authenticated user",
        "tags": ["Me Bookmarks"],
        "parameters": [
          {
            "name": "tags",
            "in": "query",
            "description": "Filter by tag slugs (comma-separated)",
            "schema": {
              "type": "string",
              "example": "tag-one,tag-two"
            }
          },
          {
            "name": "q",
            "in": "query",
            "description": "Search query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "after",
            "in": "query",
            "description": "Pagination cursor (bookmark ID)",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of bookmarks",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookmarkCollection"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "post": {
        "summary": "Create bookmark",
        "description": "Create a new bookmark",
        "tags": ["Me Bookmarks"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BookmarkCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Bookmark created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookmarkOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "422": {
            "description": "Validation error"
          }
        }
      }
    },
    "/api/users/me/bookmarks/{id}": {
      "get": {
        "summary": "Get bookmark",
        "description": "Retrieve a specific bookmark by ID",
        "tags": ["Me Bookmarks"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Bookmark details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookmarkOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Bookmark not found"
          }
        }
      },
      "patch": {
        "summary": "Update bookmark",
        "description": "Update a bookmark (URL cannot be changed)",
        "tags": ["Me Bookmarks"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BookmarkUpdate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Bookmark updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookmarkOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Bookmark not found"
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "delete": {
        "summary": "Delete bookmark",
        "description": "Delete a bookmark",
        "tags": ["Me Bookmarks"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Bookmark deleted successfully"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Bookmark not found"
          }
        }
      }
    },
    "/api/users/me/tags": {
      "get": {
        "summary": "List own tags",
        "description": "Retrieve all tags owned by the authenticated user",
        "tags": ["Me Tags"],
        "responses": {
          "200": {
            "description": "List of tags",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagCollection"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "post": {
        "summary": "Create tag",
        "description": "Create a new tag (or return existing if name matches)",
        "tags": ["Me Tags"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TagCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Tag created or retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "422": {
            "description": "Validation error or tag limit exceeded (max 1000 tags)"
          }
        }
      }
    },
    "/api/users/me/tags/{slug}": {
      "get": {
        "summary": "Get tag",
        "description": "Retrieve a specific tag by slug",
        "tags": ["Me Tags"],
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "example": "my-tag"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tag details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Tag not found"
          }
        }
      },
      "patch": {
        "summary": "Update tag",
        "description": "Update a tag",
        "tags": ["Me Tags"],
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "example": "my-tag"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TagUpdate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Tag updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagOwner"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Tag not found"
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "delete": {
        "summary": "Delete tag",
        "description": "Delete a tag",
        "tags": ["Me Tags"],
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "example": "my-tag"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Tag deleted successfully"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Tag not found"
          }
        }
      }
    },
    "/api/users/me/files": {
      "post": {
        "summary": "Upload file",
        "description": "Upload a file and receive a FileObject reference",
        "tags": ["Me Files"],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "required": ["file"],
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "File uploaded successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileObject"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/api/users/me/files/{id}": {
      "get": {
        "summary": "Get file (placeholder)",
        "description": "Placeholder route for IRI generation. Use POST /api/users/me/files to upload files.",
        "tags": ["Me Files"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "405": {
            "description": "Method not allowed"
          }
        }
      }
    },
    "/api/profile/{username}": {
      "get": {
        "summary": "Get public profile",
        "description": "Retrieve a public user profile by username",
        "tags": ["Profile"],
        "security": [],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "minLength": 3,
              "maxLength": 32,
              "example": "johndoe"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Public profile",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserProfile"
                }
              }
            }
          },
          "404": {
            "description": "Profile not found or not public"
          }
        }
      }
    },
    "/api/profile/{username}/bookmarks": {
      "get": {
        "summary": "List public bookmarks",
        "description": "Retrieve public bookmarks for a user",
        "tags": ["Profile Bookmarks"],
        "security": [],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "minLength": 3,
              "maxLength": 32,
              "example": "johndoe"
            }
          },
          {
            "name": "tags",
            "in": "query",
            "description": "Filter by tag slugs (comma-separated)",
            "schema": {
              "type": "string",
              "example": "tag-one,tag-two"
            }
          },
          {
            "name": "q",
            "in": "query",
            "description": "Search query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "after",
            "in": "query",
            "description": "Pagination cursor (bookmark ID)",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of public bookmarks",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookmarkPublicCollection"
                }
              }
            }
          },
          "404": {
            "description": "User not found or not public"
          }
        }
      }
    },
    "/api/profile/{username}/bookmarks/{id}": {
      "get": {
        "summary": "Get public bookmark",
        "description": "Retrieve a specific public bookmark",
        "tags": ["Profile Bookmarks"],
        "security": [],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "minLength": 3,
              "maxLength": 32,
              "example": "johndoe"
            }
          },
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Public bookmark details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BookmarkProfile"
                }
              }
            }
          },
          "404": {
            "description": "Bookmark not found or not public"
          }
        }
      }
    },
    "/api/profile/{username}/tags": {
      "get": {
        "summary": "List public tags",
        "description": "Retrieve public tags for a user",
        "tags": ["Profile Tags"],
        "security": [],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "minLength": 3,
              "maxLength": 32,
              "example": "johndoe"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of public tags",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagPublicCollection"
                }
              }
            }
          },
          "404": {
            "description": "User not found or not public"
          }
        }
      }
    },
    "/api/profile/{username}/tags/{slug}": {
      "get": {
        "summary": "Get public tag",
        "description": "Retrieve a specific public tag",
        "tags": ["Profile Tags"],
        "security": [],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "minLength": 3,
              "maxLength": 32,
              "example": "johndoe"
            }
          },
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "example": "my-tag"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Public tag details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagProfile"
                }
              }
            }
          },
          "404": {
            "description": "Tag not found or not public"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JWT token obtained from /api/auth"
      }
    },
    "schemas": {
      "IRI": {
        "type": "object",
        "properties": {
          "@iri": {
            "type": "string",
            "format": "uri",
            "example": "/api/users/me/tags/my-tag"
          }
        }
      },
      "UserCreate": {
        "type": "object",
        "required": ["email", "username", "password"],
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "example": "user@example.com"
          },
          "username": {
            "type": "string",
            "minLength": 3,
            "maxLength": 32,
            "example": "johndoe"
          },
          "password": {
            "type": "string",
            "format": "password",
            "minLength": 8,
            "example": "password123"
          },
          "isPublic": {
            "type": "boolean",
            "default": false,
            "example": false
          },
          "meta": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "example": {
              "theme": "dark",
              "language": "en"
            }
          }
        }
      },
      "UserUpdate": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "minLength": 3,
            "maxLength": 32,
            "example": "johndoe"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "format": "password",
            "minLength": 8,
            "example": "newpassword123"
          },
          "meta": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "description": "Meta fields are merged, not replaced",
            "example": {
              "theme": "light",
              "notifications": "enabled"
            }
          }
        }
      },
      "UserOwner": {
        "allOf": [
          {
            "$ref": "#/components/schemas/IRI"
          },
          {
            "type": "object",
            "properties": {
              "email": {
                "type": "string",
                "format": "email",
                "example": "user@example.com"
              },
              "username": {
                "type": "string",
                "example": "johndoe"
              },
              "isPublic": {
                "type": "boolean",
                "example": false
              },
              "meta": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                },
                "example": {
                  "theme": "dark",
                  "language": "en"
                }
              }
            }
          }
        ]
      },
      "UserProfile": {
        "allOf": [
          {
            "$ref": "#/components/schemas/IRI"
          },
          {
            "type": "object",
            "properties": {
              "username": {
                "type": "string",
                "example": "johndoe"
              }
            }
          }
        ]
      },
      "BookmarkCreate": {
        "type": "object",
        "required": ["title", "url"],
        "properties": {
          "title": {
            "type": "string",
            "example": "Example Website"
          },
          "url": {
            "type": "string",
            "format": "uri",
            "example": "https://example.com"
          },
          "isPublic": {
            "type": "boolean",
            "default": false,
            "example": false
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri",
              "description": "IRI reference to tag (e.g., /api/users/me/tags/my-tag)"
            },
            "example": [
              "/api/users/me/tags/web-development",
              "/api/users/me/tags/tutorials"
            ]
          },
          "mainImage": {
            "type": "string",
            "format": "uri",
            "nullable": true,
            "description": "IRI reference to FileObject",
            "example": "/api/users/me/files/123e4567-e89b-12d3-a456-426614174000"
          },
          "archive": {
            "type": "string",
            "format": "uri",
            "nullable": true,
            "description": "IRI reference to FileObject",
            "example": "/api/users/me/files/123e4567-e89b-12d3-a456-426614174001"
          }
        }
      },
      "BookmarkUpdate": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "Updated Title"
          },
          "isPublic": {
            "type": "boolean",
            "example": true
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            },
            "example": [
              "/api/users/me/tags/web-development"
            ]
          },
          "mainImage": {
            "type": "string",
            "format": "uri",
            "nullable": true
          },
          "archive": {
            "type": "string",
            "format": "uri",
            "nullable": true
          }
        }
      },
      "BookmarkOwner": {
        "allOf": [
          {
            "$ref": "#/components/schemas/IRI"
          },
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "format": "uuid",
                "example": "123e4567-e89b-12d3-a456-426614174000"
              },
              "createdAt": {
                "type": "string",
                "format": "date-time",
